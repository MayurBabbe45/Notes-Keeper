# Server Configuration
REACT_APP_API_URL=https://api.example.com
REACT_APP_API_KEY=your-api-key-here
REACT_APP_API_SECRET=your-api-secret-here

# Environment Configuration
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# Authentication
REACT_APP_AUTH_DOMAIN=your-auth-domain.com
REACT_APP_AUTH_CLIENT_ID=your-client-id
REACT_APP_AUTH_CLIENT_SECRET=your-client-secret

# Firebase Configuration (if using Firebase)
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-firebase-auth-domain
REACT_APP_FIREBASE_PROJECT_ID=your-firebase-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-firebase-storage-bucket
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-firebase-messaging-sender-id
REACT_APP_FIREBASE_APP_ID=your-firebase-app-id

# Google Analytics (if using Google Analytics)
REACT_APP_GA_TRACKING_ID=your-google-analytics-tracking-id

# Sentry (for error tracking)
REACT_APP_SENTRY_DSN=your-sentry-dsn

# Feature Toggles
REACT_APP_FEATURE_X_ENABLED=false
REACT_APP_FEATURE_Y_ENABLED=true

# Other Environment Variables
REACT_APP_SUPPORT_EMAIL=support@example.com
REACT_APP_CONTACT_PHONE=+1234567890

# Third-Party Service Keys
REACT_APP_STRIPE_PUBLIC_KEY=your-stripe-public-key
REACT_APP_MAPBOX_TOKEN=your-mapbox-token
REACT_APP_PAYPAL_CLIENT_ID=your-paypal-client-id

# Miscellaneous
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_MAX_UPLOAD_SIZE=10485760 # in bytes (10 MB)
REACT_APP_TIMEZONE=UTC
